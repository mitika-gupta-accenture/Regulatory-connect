  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "env-cmd -f .env.development react-scripts build",
    "start:uat": "env-cmd -f .env.uat react-scripts start",
    "build:uat": "env-cmd -f .env.uat react-scripts build",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "test": "jest",
    "test:single": "jest src/core/hooks/__tests__/useEventHandlers.test.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-eval": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
